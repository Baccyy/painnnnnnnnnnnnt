{
  "name": "paint-bot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "bot": "tsc && dotenv -e .env node built/bot.js",
    "dev": "tsc && dotenv -e .env.development node built/bot.js",
    "commands": "tsc && dotenv -e .env node built/botCommands.js",
    "commands:dev": "tsc && dotenv -e .env.development node built/botCommands.js",
    "guilds": "tsc && dotenv -e .env node built/botGuilds.js",
    "guilds:dev": "tsc && dotenv -e .env.development node built/botGuilds.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cryptr": "^6.1.0",
    "discord.js": "^14.7.1",
    "express": "^4.18.2",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.15",
    "@types/node": "^12.0.0",
    "dotenv-cli": "^5.1.0"
  }
}
